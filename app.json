{
  "expo": {
    "name": "LeadSystem",
    "slug": "LeadSystem",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/applogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00000000"
    },
    "notification":{
      "icon":"./assets/applogo.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leadsystem",
      "buildNumber": "9",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/applogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Otacon.LeadSystem",
      "versionCode": 4,
      "permissions": ["NOTIFICATIONS"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c893217e-1758-46d2-b886-0bb87fa90020"
      }
    },
    "runtimeVersion": "1.0.4",
    "updates": {
      "url": "https://u.expo.dev/c893217e-1758-46d2-b886-0bb87fa90020"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/applogo.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ]
    ]
  }
}
